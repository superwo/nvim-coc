ease: "power4.out"
});
}
endsnippet

snippet gscroll "gsap scroll" b
import gsap from 'gsap';
import ScrollTrigger from "gsap/ScrollTrigger";
export default function $1() {
	gsap.registerPlugin(ScrollTrigger);
	let tl = gsap.timeline();
	tl.from('.logo-svg-left svg', {
opacity: 0,
scale: 300,
duration: 1.5,
ease: "power4.out"
});
}
endsnippet
