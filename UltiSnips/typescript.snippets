snippet gen_func "generate function" b
const elems = [
	{
	el: '$1',
				functions: [$2],
	},
]
for (const elem of elems) {
	const el = document.querySelector(elem.el);
	if (el && elem.functions) {
		for (const func of elem.functions) {
			func();
		}
	}
}
endsnippet

snippet gfixed "gsap fixed" b
